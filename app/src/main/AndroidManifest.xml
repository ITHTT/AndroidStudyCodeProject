<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ithtt.androidstudy">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AndroidCodeRecyclerViewActivity"/>

        <activity android:name=".androidview.AndroidViewActivity"/>

        <activity android:name=".androidview.customview.dashboardview.DashBoardActivity"/>

        <activity android:name=".androidview.customview.clockview.ClockViewActivity"/>

        <activity android:name=".androidview.customview.pageviewtransition.PagerViewTransitionActivity"/>

        <activity android:name=".androidview.customview.flikerprogressbar.FlikerProgressBarActivity"/>

        <activity android:name=".androidview.customview.pyramidview.PyramidViewActivity"/>

        <activity android:name=".androidview.customview.meizubanner.MeiZuBannerActivity"/>

        <activity android:name=".androidview.customview.bezierview.BezierViewActivity"/>

        <activity android:name=".androidview.customview.chartview.ChartViewActivity"/>

        <activity android:name=".androidview.customview.marqueeview.MarqueeViewActivity"/>

        <activity android:name=".androidview.customview.animtextview.AnimTextViewActivity"/>

        <activity android:name=".androidview.systemview.recyclerview.RecyclerViewActivity"/>

        <activity android:name=".androidview.systemview.recyclerview.LinearLayoutActivity"/>

        <activity android:name=".androidview.systemview.recyclerview.GridLayoutActivity"/>

        <activity android:name=".androidview.systemview.recyclerview.StaggeredGridLayoutActivity"/>
    </application>

</manifest>